<div class="widget-body">

    <div class="row innerAll inner-2x">
        <div class="col-sm-6">
            <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %>
            <%= devise_error_messages! %>
                <div class="form-group">
                    <%= f.label :firstname %>
                     <%= f.text_field :firstname, :id => 'lastname', :class => 'form-control' %>
                </div>

                <div class="form-group">
                    <%= f.label :lastname %>
                     <%= f.text_field :lastname, :id => 'lastname', :class => 'form-control' %>
                </div>

                <div class="form-group">
                    <%= f.label :username %>
                     <%= f.text_field :username, :id => 'username', :class => 'form-control' %>
                </div>

                <div class="form-group">
                    <%= f.label :phone %>
                    <%= f.text_field :phone, :id => 'phone', :class => 'form-control' %>
                </div>

                <div class="form-group">
                    <%= f.label :email %>
                    <%= f.email_field :email, :id => 'email', :class => 'form-control' %>
                </div>

                <div class="form-group">
                    <%= f.label :address %>
                     <%= f.text_field :address, :id => 'address', :class => 'form-control' %>
                </div>

                <div class="form-group">
                    <%= f.label :password %> <i>(leave blank if you don't want to change it)</i>
                    <%= f.password_field :password, autocomplete: "off", :class => 'form-control' %>
                </div>

                <div class="form-group">
                    <%= f.label :password_confirmation %>
                    <%= f.password_field :password_confirmation, autocomplete: "off", :class => 'form-control' %>
                </div>

                <div class="form-group">
                    <%= f.label :current_password %> <i>(we need your current password to confirm your changes)</i>
                    <%= f.password_field :current_password, autocomplete: "off", :class => 'form-control' %>
                </div>

                <div class="checkbox">
                    <label for="notify">
                        <input id="notify" checked="checked" name="notify" type="checkbox">
                        Email me with updates about current projects.
                    </label>
                </div>

                <div class="separator"></div>
                  <div class="form-group">
                    <%= f.submit "Update", :class => 'btn btn-primary' %>
                  </div>
            <% end %>
        </div>
        <div class="col-sm-6">
            <div class="well">
                <h4>Additional Information</h4>
                <div class="separator"></div>
